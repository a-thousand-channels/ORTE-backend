@charset "UTF-8";

@font-face {
  font-family: "orte";
  src:asset-url('orte/orte.eot');
  src:asset-url('orte/orte.eot?#iefix') format("embedded-opentype"),
    asset-url('orte/orte.woff') format("woff"),
    asset-url('orte/orte.ttf') format("truetype"),
    asset-url('orte/orte.svg#orte') format("svg");
  font-weight: normal;
  font-style: normal;

}

[data-icon]:before {
  font-family: "orte" !important;
  content: attr(data-icon);
  font-style: normal !important;
  font-weight: normal !important;
  font-variant: normal !important;
  text-transform: none !important;
  speak: none;
  line-height: 1;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

[class^="orte-icons-"]:before,
[class*=" orte-icons-"]:before {
  font-family: "orte" !important;
  font-style: normal !important;
  font-weight: normal !important;
  font-variant: normal !important;
  text-transform: none !important;
  speak: none;
  line-height: 0.8;
  font-size: 32px;
  vertical-align: top;  
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.orte-icons-maps:before {
  content: "\62";
}
.orte-icons-layers:before {
  content: "\63";
}
.orte-icons-places:before {
  content: "\61";
}
